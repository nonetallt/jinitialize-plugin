    "type": "project",
    "keywords": [],
    "require": {
        "php": ">=7.0.0",
        "nonetallt/jinitialize-core": "dev-master",
        "symfony/console": "^3.4"
    },
    "require-dev": {
        "phpunit\/phpunit": "^6",
        "consolidation\/robo": "~1",
        "henrikbjorn\/lurker": "^1.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jyri Mikkola",
            "email": "jyri.mikkola@pp.inet.fi"
        }
